<view class="container"><view data-event-opts="{{[['tap',[['serGoods']]]]}}" class="mp-search-box" bindtap="__e"><image class="icon_b" src="../../static/icon_b.png"></image><input class="ser-input" type="text" placeholder="搜索你需要的商品热门关键词" disabled/></view><view class="nocCss">{{"通告:"+nocStr}}</view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['carouselList','',index,'goodsId']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.img}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="labelCss"><view class="labelView"><image class="imgLabel" src="../../static/baoyou.png"></image><label class="labeltxt">顺丰包邮</label></view><view class="labelView"><image class="imgLabel" src="../../static/baozhang.png"></image><label class="labeltxt">正品保障</label></view><view class="labelView"><image class="imgLabel" src="../../static/shengqian.png"></image><label class="labeltxt">多快好省</label></view></view><view class="cate-section"><block wx:for="{{menus}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToCatePage',['$0','$1'],[[['menus','',index,'id']],[['menus','',index,'parentId']]]]]]]}}" class="cate-item" bindtap="__e"><image src="{{item.icon}}"></image><text class="txtmenu">{{item.name}}</text></view></block></view><view class="f-header m-t"><view class="tit-box"><text class="tit">猜你喜欢</text></view><text></text></view><view class="guess-section"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="guess-item"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index,'id']]]]]]]}}" class="image-wrapper" bindtap="__e"><image src="{{item.coverImg[0]}}" mode="aspectFill"></image></view><text class="title clamp">{{item.name}}</text><text class="author">{{item.author}}</text><view class="viewGood"><view class="viewPrice"><text class="priceNow">{{"￥"+item.price}}</text><text class="priceOld">{{"￥"+item.cost}}</text></view><image class="goodBuy" src="../../static/goumai.png" data-event-opts="{{[['tap',[['car',['$0'],[[['goodsList','',index,'id']]]]]]]}}" bindtap="__e"></image></view></view></block></view></view>