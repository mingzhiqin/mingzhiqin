<view class="content"><view class="row b-b"><text class="tit">收货人姓名</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','nickname','$event',[]],['addressData']]]]]}}" value="{{addressData.nickname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号码</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['addressData']]]]]}}" value="{{addressData.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">选择省市</text><block wx:for="{{selectList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['togglePopup',['bottom','popup']]]]]}}" class="txtloc" bindtap="__e">{{''+item.txt}}<label hidden="{{!(index==0||index==1)}}" class="_span">—</label></view></block></view><uni-popup class="vue-ref" vue-id="1" type="{{type}}" data-ref="popup" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="select-border"><view class="header"><view class="title">选择地区</view><view data-event-opts="{{[['tap',[['cancel',['popup']]]]]}}" class="cancel-icon" bindtap="__e">X</view></view><view class="select-box"><view class="select-item"><block wx:for="{{selectList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabEvent',[index]]]]]}}" class="{{['select-list',indexTab==index?'selected':'']}}" bindtap="__e">{{''+item.txt+''}}</view></block></view><view class="select-item-box"><view hidden="{{!(proviceShow)}}" class="province-box"><block wx:for="{{provinceData}}" wx:for-item="item" wx:for-index="index" wx:key="code"><view data-event-opts="{{[['tap',[['provinceEvent',['$0',index],[[['provinceData','code',item.code]]]]]]]}}" class="select-list-cont" bindtap="__e">{{''+item.name}}<label hidden="{{!(index==checkOne)}}" class="check _span">√</label></view></block></view><view hidden="{{!(cityShow)}}" class="city-box"><block wx:for="{{cityData}}" wx:for-item="item" wx:for-index="index" wx:key="code"><view data-event-opts="{{[['tap',[['cityEvent',['$0',index],[[['cityData','code',item.code]]]]]]]}}" class="select-list-cont" bindtap="__e">{{''+item.name}}<label hidden="{{!(index==checkTwo)}}" class="check _span">√</label></view></block></view><view hidden="{{!(areaShow)}}" class="area-box"><block wx:for="{{areaData}}" wx:for-item="item" wx:for-index="index" wx:key="code"><view data-event-opts="{{[['tap',[['areaEvent',['$0',index],[[['areaData','code',item.code]]]]]]]}}" class="select-list-cont" bindtap="__e">{{''+item.name}}<label hidden="{{!(index==checkThree)}}" class="check _span">√</label></view></block></view></view></view></view></uni-popup><view class="row b-b"><text class="tit">街道地址</text><input class="input" type="text" placeholder="街道、楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addressData']]]]]}}" value="{{addressData.address}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">邮政编码</text><input class="input" type="text" placeholder="请输入邮政编码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','postcode','$event',[]],['addressData']]]]]}}" value="{{addressData.postcode}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.isDefault}}" color="#333233" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>